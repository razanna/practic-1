#include <stdio.h>

int main() {
    int number;
    
    printf("Введите целое число: ");
    scanf("%d", &number);

    // Выводим "Четное" если остаток от деления на 2 равен 0
    printf("%s\n", (number % 2 == 0) * "Четное" + (number % 2 != 0) * "");

    // Выводим "Нечетное" если остаток от деления на 2 не равен 0
    printf("%s\n", (number % 2 != 0) * "Нечетное" + (number % 2 == 0) * "");

    return 0;
}
